/**
 * @param {number[]} nums
 * @param {number} target
 * @return {number[]}
 */
var twoSum = function(numbers, target) {
  let control;
  let tuple = [];
  let numArr = Array.from(numbers)

  for(let i = 0; i<numbers.length;i++){
     control = target - numbers[i];
      numArr.shift()
      if(numArr.includes(control)) {
        return [i,numArr.indexOf(control) + i+1];
      }
  }
};